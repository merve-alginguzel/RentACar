
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Mouldifi - A fully responsive, HTML5 based admin theme">
    <meta name="keywords" content="Responsive, HTML5, admin theme, business, professional, Mouldifi, web design, CSS3">
    <title>@ViewBag.Title</title>
    <!-- Site favicon -->
    <link rel='shortcut icon' type='image/x-icon' href='images/favicon.ico' />
    <!-- /site favicon -->
    <!-- Entypo font stylesheet -->
    <link href="~/Content/css/entypo.css" rel="stylesheet">
    <!-- /entypo font stylesheet -->
    <!-- Font awesome stylesheet -->
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet">
    <!-- /font awesome stylesheet -->
    <!-- CSS3 Animate It Plugin Stylesheet -->
    <link href="~/Content/css/plugins/css3-animate-it-plugin/animations.css" rel="stylesheet">
    <!-- /css3 animate it plugin stylesheet -->
    <!-- Bootstrap stylesheet min version -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <!-- /bootstrap stylesheet min version -->
    <!-- Mouldifi core stylesheet -->
    <link href="~/Content/css/mouldifi-core.css" rel="stylesheet">
    <!-- /mouldifi core stylesheet -->

    <link href="~/Content/css/plugins/select2/select2.css" rel="stylesheet">

    <link href="~/Content/css/mouldifi-forms.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="~/Content/js/html5shiv.min.js"></script>
          <script src="~/Content/js/respond.min.js"></script>
    <![endif]-->


</head>
<body>

    <!-- Page container -->
    <div class="page-container">
        <div class="page-sidebar">

            <!-- Site header  -->
            <header class="site-header">
                <div class="site-logo"><a href="index.html">MERVE ALGINGÜZEL</a></div>
                <div class="sidebar-collapse hidden-xs"><a class="sidebar-collapse-icon" href="#"><i class="icon-menu"></i></a></div>
                <div class="sidebar-mobile-menu visible-xs"><a data-target="#side-nav" data-toggle="collapse" class="mobile-menu-icon" href="#"><i class="icon-menu"></i></a></div>
            </header>
            <!-- /site header -->
            <!-- Main navigation -->
            <ul id="side-nav" class="main-menu navbar-collapse collapse">
                <li>
                    <a href="@Url.Action("CarRecord","Home")"><i class="fa fa-car"></i><span class="title">ARAÇ EKLE</span></a>
                </li>
                <li>
                    <a href="@Url.Action("Customer","Home")"><i class="fa fa-user"></i><span class="title">MÜŞTERİLER EKLE</span></a>

                </li>
                <li>
                    <a href="@Url.Action("ReservationAdd","Home")"><i class="fa fa-check-square-o"></i><span class="title">REZERVASYON EKLE</span></a>
                </li>
                <li class="has-sub">
                    <a href=""><i class="fa fa-yelp"></i><span class="title">RAPORLAR</span></a>
                    <ul class="nav collapse">
                        <li><a href="@Url.Action("Reservation","Home")"><span class="title">Rezervasyonlar</span></a></li>
                        <li><a href="@Url.Action("getByDateCar","Home")"><span class="title">Tarihlere Göre Araç Listesi</span></a></li>
                        <li><a href="@Url.Action("PopulerCar","Home")"><span class="title">Popüler Araçlar</span></a></li>
                    </ul>
                </li>
            </ul>
            <!-- /main navigation -->
        </div>

        @RenderBody()


    </div>



    <script src="~/Content/js/jquery.min.js"></script>
        <!-- Load CSS3 Animate It Plugin JS -->
        <script src="~/Content/js/plugins/css3-animate-it-plugin/css3-animate-it.js"></script>
        <script src="~/Content/js/bootstrap.min.js"></script>
        <script src="~/Content/js/plugins/metismenu/jquery.metisMenu.js"></script>
        <!-- Select2-->
        <script src="~/Content/js/plugins/select2/select2.full.min.js"></script>
        <script src="~/Content/js/functions.js"></script>
        <script src="~/Content/js/plugins/colorpicker/bootstrap-colorpicker.min.js"></script>
        <!--Bootstrap DatePicker-->
        <script src="~/Content/js/plugins/datepicker/bootstrap-datepicker.js"></script>

    <script>
        $(document).ready(function () {
            $(".select2").select2();
        });
    </script>

    @RenderSection("scripts", required: false)

</body>

</html>
